#include <iostream>
using namespace std;
int main()
{
  int n;
  cout<<"Vvedite N"<<endl;
  cin>>n;
  int a[n];
  cout<<"Vvedite massiv"<<endl;
  for (int i=0; i<n; i++)
  {
    cin>>a[i];
  }
  int b[n];
  cout<<"Vvedite massiv"<<endl;
  for (int i=0; i<n; i++)
  {
    cin>>b[i];
  }
  int c[n];
  c[n]=a[n];
  a[n]=b[n];
  b[n]=c[n];
  for (int i=0; i<n; i++)
  {
    cout<<a[i];
  }
  for (int i=0; i<n; i++)
  {
    cout<<b[i];
  }
  return 0;
}
